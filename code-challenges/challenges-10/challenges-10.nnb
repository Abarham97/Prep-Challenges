            "source": [
                "function migratoryBirds(arr) {\n    // Write your code here\n    const birdCounts = {};\n\n \n  for (let bird of arr) {\n    if (birdCounts[bird]) {\n      birdCounts[bird]++;\n    } else {\n      birdCounts[bird] = 1;\n    }\n  }\n\n  let mostFrequentBird = null;\n  let maxCount = 0;\n\n  \n  for (let bird in birdCounts) {\n    if (birdCounts[bird] > maxCount) {\n      mostFrequentBird = bird;\n      maxCount = birdCounts[bird];\n    }\n  }\n\n  return mostFrequentBird;\n\n}"
